* 
	margin 0 
	padding 0
	overflow none 

body
	margin 0 
	padding 0 
    font 13px/1.55 OpenSansRegular
    color #595959 
    text-align left
    background #fff    
    overflow-x hidden
img
    border 0
    vertical-align top
a
    color #1d4c7a 
    text-decoration none
a:hover
	color #1d4c7a 
	text-decoration none
a:focus 
	outline none
a:focus
	outline none
/* Headings */
h1,h2,h3,
h4,h5,h6
	margin 0 0 5px 
    line-height 1.35 
    color #666    
h1
    font-size 20px
    font-weight normal
h2
    font-size 18px 
    font-weight normal
h3 
	font-size 16px 
	font-weight normal
h4 
	font-size 14px
	font-weight normal
h5 
	font-size 12px 
	font-weight normal
h6   
	font-size 11px 
	font-weight normal
	
/*Header*/
.datos_empresa_bg
	margin 0
	background #fff
	color #595959
	padding-top 5px
	overflow-x hidden
.datos_empresa
	display -webkit-inline-box
	p 
		padding-left 2%
		span 
			color #1d4c7a
		
/*Fin Headder*/
/*Logo - Busqueda - Menu Principal*/
.logo_bg
	background #f7f7f7
	margin-left 0px
	margin-right 0px
.logo 
	margin-top 2%
	margin-bottom 2%
.buscar 
	margin-top 3%
	margin-bottom 1%
.menu 
	background none
	border none 
	-webkit-box-shadow none
.menu_left 
	margin 0 
	padding 0
.nav navbar-nav
	clear both
.navbar-default .navbar-nav>li>a 
	color #1d4c7a
	font-weight bold
.navbar-default .navbar-nav>li>a:hover 
	color #428bca
/*Fin Logo y Busqueda*/
/* Slider */
.slider_container
	//background red
.slider 
	//height 300px 
	//background black 
	.carousel-inner 
		position relative
		width 100%
		overflow hidden
		height 350px
	.diapo_l
		//background red 
	.diapo_r
		//background red 
		height 350px
		//margin-left 0
		//margin-right 0
		//padding-left 0
		//padding-right 0
		.productos_diapo
			margin 0 
			padding 0
			img 
				width 100%
/*Fin Slider*/
/*Contenedor Principal --- sidebar ----  Main*/
.contenedor_principal 
	margin-top 2%
.productos_con 
	margin 0
	padding 0
	.producto 
		margin 0 
		padding 0 5px 0 5px
	.producto_img 
		width 60%
/*Fin Sidebar*/

/*Principales*/
.principales
	margin-top 10px
	h3
		font-size 20px 
		font-weight bold
/*footer*/
.footer 
	margin-top 2%
	padding-top 2%
	background #f7f7f7
.copy 
	background #f8f8f8
	color #000
	.container
		p 
			font-weight bold
			text-align center
			font-size 16px